{
  "name": "chartpress-frontend",
  "version": "1.0.0",
  "homepage": "/chartpress/",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.0.2",
    "asciidoctor": "^3.0.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "^5.0.1",
    "react-slick": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "webpack": "^5.99.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run copy-docs && react-scripts build -v",
    "copy-docs": "rm -rf public/docs/* && mkdir -p public/docs && cp -v -R ./docs/* ./public/docs/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
